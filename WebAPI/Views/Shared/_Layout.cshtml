<!doctype html>
<html lang="en">

<head>
    <title>Flight Tracker</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta name="description" content="Lucid Bootstrap 4.1.1 Admin Template">
    <meta name="author" content="WrapTheme, design by: ThemeMakker.com">

    <link rel="icon" href="~/assets/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="~/assets/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/assets/vendor/sweetalert/sweetalert.css" />
    <link href="~/assets/vendor/toastr/toastr.min.css" rel="stylesheet" />

    @RenderSection("styles", false)

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="~/assets/css/main.css">
    <link rel="stylesheet" href="~/assets/css/color_skins.css">
    <link href="~/assets/css/custom.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>
<body class="theme-blue">

    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30"><img src="~/assets/images/travel.svg" width="48" height="48" alt="Flight Tracker"></div>
            <p style="color:green">Please wait...</p>
        </div>
    </div>
    <!-- Overlay For Sidebars -->

    <div id="wrapper">
        <div id="main-content">
            <div class="container-fluid">
                @RenderBody()
            </div>


            <div class="footer" style="text-align:center; align-content:center;display:block;">

                <div class="row" style="display:block;">
                    <span>
                        <span class="font-weight-bold"></span>
                            Test Application  
                    </span>
                </div>
            </div>
        </div>

    </div>

    <!-- #region model views -->


    <div class="modal" id="myModalAddOn" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog " style="width:1124px">
            <div class="modal-content">
               
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <div class="modal" id="myModalAddInfo" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog " style="width:1124px">
            <div class="modal-content">
              
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>



    <!-- #endregion -->
    <!-- Javascript -->
    <script src="~/assets/bundles/libscripts.bundle.js"></script>
    <script src="~/assets/bundles/vendorscripts.bundle.js"></script>
    <script src="~/assets/bundles/mainscripts.bundle.js"></script>
    <script src="~/assets/vendor/toastr/toastr.js"></script>
    <script src="~/assets/vendor/sweetalert/sweetalert.min.js"></script>

    <script src="~/assets/js/tableHelper.js"></script>

    @RenderSection("scripts", false)

</body>
</html>



